<% @usuarios.each do |usuario| %>
  Nombres <%= usuario.nombres %>
  Apellidos <%= usuario.apellidos %>
  Rut <%= usuario.rut.to_s %>
  Email <%= usuario.email %>
  Contraseña <%= usuario.password %>
  Fecha_nacimiento <%= usuario.fecha_nacimiento %>
  Direccion <%= usuario.direccion %>

  comuna <%= usuario.comuna.descripcion %>
  tipo_usuario <%= usuario.tipo_usuario.descripcion %>
  <%=link_to 'mostrar', usuario_url(usuario) %>
  <%= link_to 'editar', editar_usuario_url(usuario) %>
  <%= link_to 'eliminar', registration_path(usuario), data: { confirm: "Estás seguro?" }, method: :delete %>
  <br>
<%end %>