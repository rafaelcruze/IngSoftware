<div class="login-form">
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="form-group">
      <h1 class="text-center "><i class="fas fa-user"></i></h1><br>
  <div class="field">
    <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder:"Email", class:"form-control" %>
  </div>
    </div>
  <div class="form-group">
  <div class="field">
    <%= f.password_field :password, autocomplete: "off", placeholder:"Password", class:"form-control" %>
  </div>
  </div>
    <div class="form-group">
      <div class="actions">
        <%= f.submit "Iniciar Sesion", class: "btn btn-primary btn-block" %>
      </div>
    </div>
  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me, class:"pull-left checkbox-inline" %>
      <%= render "devise/shared/links" , class:"pull-right"%>
    </div>
  <% end %>
<% end %>
</div>

